@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns#>.

<#TriplesMap1>
    rr:logicalTable [ rr:tableName "mockcsv.\"cities.csv\"" ];
    rr:subjectMap [
        rr:template "http://data.example.com/employee/{City}";
        rr:class ex:City;
    ];
    rr:predicateObjectMap [
        rr:predicate ex:state;
        rr:objectMap [ rr:column "State" ];
    ].
